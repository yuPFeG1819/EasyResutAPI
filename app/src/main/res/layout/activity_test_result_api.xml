<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <variable
            name="config"
            type="com.yupfeg.resulttest.TestResultApiActivity.BindingConfig"
            />
    </data>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >

        <Button
            android:id="@+id/btn_test_result_api_normal_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="正常返回"
            onViewSingleClick="@{()->config.normalBack()}"
            />

        <Button
            android:id="@+id/btn_test_result_api_result_data"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="set_Result返回"
            android:textAllCaps="false"
            onViewSingleClick="@{()->config.resultDataBack()}"
            />
    </LinearLayout>
</layout>
